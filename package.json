    "we-plugin-editor-tinymce": "^1.2.10",
    "we-plugin-email": "^2.1.1",
    "we-plugin-field-phone": "^1.1.0",
    "we-plugin-file": "^3.1.0",
    "we-plugin-file-local": "2.1.5",
    "we-plugin-form": "^1.4.1",
    "we-plugin-google-analytics": "^1.3.0",
    "we-plugin-i18n-api": "^1.1.0",
    "we-plugin-mailchimp-sb": "^1.0.2",
