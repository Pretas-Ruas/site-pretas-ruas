    "we-plugin-user-settings": "^2.0.3",
    "we-plugin-view": "^2.0.4",
    "we-plugin-vocabulary": "^3.0.0",
    "we-plugin-widget": "^3.0.0",
    "winston": "3.3.3",
    "we-plugin-gallery": "^1.1.2",
    "we-admin-ong": "linkysystems/we-admin-ong#v1.3.8"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2",
    "supervisor": "^0.12.0",
    "we-gulp-tasks-default": "^2.0.1",
    "we-test-tools": "^1.0.0"
  },
