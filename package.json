    "lodash": "^4.17.20",
    "mysql2": "^2.1.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "we": "^1.5.10",
    "we-core": "^3.1.14",
    "we-passport-oauth2-password": "^2.0.2",
    "we-plugin-acl": "^1.2.16",
    "we-plugin-auth": "^2.3.4",
    "we-plugin-comment": "^3.1.0",
    "we-plugin-contact": "^2.0.0",
    "we-plugin-csv": "^1.1.1",
